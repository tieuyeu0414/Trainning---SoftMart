    "react-transition-group": "^4.4.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build-css": "node-sass --output-style compressed -o src/assets/styles/css/compressed src/assets/styles/scss",
    "watch-css": "npm run build-css && node-sass --output-style compressed -o src/assets/styles/css/compressed src/assets/styles/scss --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build:ST": "env-cmd -f .env.ST  react-scripts build",
    "build:SIT": "env-cmd -f .env.SIT  react-scripts build",
    "build:UAT": "env-cmd -f .env.UAT react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "npm-run-all": "^4.1.5"
  }
}
